<?xml version="1.0" encoding="utf-8" ?>
<Document Version="0.0.1">
  <Entities>
    <Entity Name="Directory">
      <Declarations>
        <Property Name="Path" IsRequired="true" />
        <Property Name="Language" IsRequired="true" />
      </Declarations>
      <Records>
        <PrefixMany Path="..\..\..\..\Snippetica\Snippetica">
          <Set Language="CSharp">
            <New Path=".CSharp" />
          </Set>
          <Set Language="VisualBasic">
            <New Path=".VisualBasic" />
          </Set>
          <Set Language="Cpp">
            <New Path=".Cpp" />
          </Set>
          <Set Language="Html">
            <New Path=".Html" Tag="GenerateXmlSnippets" />
          </Set>
          <Set Language="Xaml">
            <New Path=".Xaml">
              <Tag Value="GenerateXmlSnippets" />
              <Tag Value="ExcludeFromVisualStudioCode" />
            </New>
          </Set>
          <Set Language="Xml">
            <New Path=".Xml" Tag="GenerateXmlSnippets" />
            <New Path=".Xml.CodeSnippet">
              <Tag Value="ExcludeFromVisualStudioCode" />
              <Tag Value="NoQuickReference" />
            </New>
          </Set>
        </PrefixMany>
      </Records>
    </Entity>
  </Entities>
</Document>
